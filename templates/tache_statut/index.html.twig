{% extends 'base.html.twig' %}

{% block title %}TacheStatut index{% endblock %}

{% block body %}
    <h1>TacheStatut index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>DateChangement</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for tache_statut in tache_statuts %}
            <tr>
                <td>{{ tache_statut.id }}</td>
                <td>{{ tache_statut.dateChangement ? tache_statut.dateChangement|date('Y-m-d') : '' }}</td>
                <td>
                    <a href="{{ path('app_tache_statut_show', {'id': tache_statut.id}) }}">show</a>
                    <a href="{{ path('app_tache_statut_edit', {'id': tache_statut.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_tache_statut_new') }}">Create new</a>
{% endblock %}
